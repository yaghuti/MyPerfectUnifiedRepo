    "dataSources": {
        "ds_0SEcxnrn": {
            "name": "risk_events",
            "options": {
                "enableSmartSources": true,
                "query": "| from datamodel Risk.All_Risk | rename normalized_risk_object AS dest_ip | search dest_ip = $dest_ip$\n| stats count by search_name"
            },
            "type": "ds.search"
        },
        "ds_70zt6tTO": {
            "name": "affected_hosts",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| search threat=\"$threat_token$\"\n| stats count by dest_ip\n| rename dest_ip as \"Host IP Address\"\n| table \"Host IP Address\""
            },
            "type": "ds.chain"
        },
        "ds_7LZlDEZh": {
            "name": "Top Source IP count",
            "options": {
                "query": "`cisco_secure_firewall` EventType=ConnectionEvent dest_ip = $dest_ip$\n| stats count BY  dest_ip src_ip connection_id | sort count"
            },
            "type": "ds.search"
        },
        "ds_7sRMJ3vQ": {
            "name": "snort_ids_threat_actor_lookup",
            "options": {
                "query": "|inputlookup cisco_snort_ids_to_threat_mapping |  table threat signature_id category message \n|  stats values(signature_id) as signature_id  values(category) as category by threat"
            },
            "type": "ds.search"
        },
        "ds_8LU6nawL": {
            "name": "all_intrusion_events_by_threat_actors",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| stats count by threat"
            },
            "type": "ds.chain"
        },
        "ds_8sclv3pU": {
            "name": "timeline_instruction",
            "options": {
                "query": "`cisco_secure_firewall` EventType=IntrusionEvent dest_ip = \"$dest_ip$\"\n| timechart span=1d count BY signature_id"
            },
            "type": "ds.search"
        },
        "ds_PpNvhTDF": {
            "name": "dc_signature",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| search threat=\"$threat_token$\"\n| stats dc(signature)"
            },
            "type": "ds.chain"
        },
        "ds_Q6zbF7Qb": {
            "name": "Top Signatures",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| search threat=\"$threat_token$\"\n| stats count  by signature | table signature "
            },
            "type": "ds.chain"
        },
        "ds_QAFOBzEx": {
            "name": "volume_inbound",
            "options": {
                "query": "`cisco_secure_firewall` EventType=ConnectionEvent dest_ip=\"$dest_ip$\"\n| stats sum(InitiatorBytes) AS inbound_bytes\n| eval Volume_Inbound_GB = round(inbound_bytes/1024/1024/1024, 2) . \" Gb\"\n| fields Volume_Inbound_GB"
            },
            "type": "ds.search"
        },
        "ds_TPwx4BWO": {
            "name": "affected_dest_ip",
            "options": {
                "enableSmartSources": true,
                "query": "`cisco_secure_firewall` EventType=IntrusionEvent \n| bin _time span=1d \n| stats count as Total_Alerts dc(signature_id) as sig_count \n    values(signature_id) as signature_ids \n    values(signature) as signature \n    values(class_desc) as class_desc \n    values(MitreAttackGroups) as MitreAttackGroups \n    values(InlineResult) as InlineResult \n    values(InlineResultReason) as InlineResultReason \n    values(src_ip) as src_ip \n    values(dest_port) as dest_port \n    values(rule) as rule \n    values(transport) as transport \n    values(connection_id) as connection_id\n    values(app) as app\n    by dest_ip _time \n| eval sig_ids_str = mvjoin(signature_ids, \",\") \n| lookup cisco_snort_ids_to_threat_mapping signature_id as sig_ids_str OUTPUT threat_actor, distinct_count_snort_ids, description \n| where isnotnull(threat_actor) AND sig_count >= tonumber(distinct_count_snort_ids) \n| `security_content_ctime(firstTime)` \n| `security_content_ctime(lastTime)`\n| search threat_actor = \"$threat_actor_token$\"\n| table _time threat_actor description dest_ip signature_ids signature src_ip MitreAttackGroups\n| stats count by dest_ip\n| rename dest_ip as \"Private IP Address\"\n| table \"Private IP Address\"",
                "queryParameters": {
                    "earliest": "$global_time.earliest$",
                    "latest": "$global_time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_ULaiHv6R": {
            "name": "risk_event_timeline",
            "options": {
                "query": "index=risk normalized_risk_object=\"$dest_ip$\"\n| bin _time span=1d\n| stats count AS event_count BY _time\n| timechart span=1d sum(event_count) AS \"Events per Day\""
            },
            "type": "ds.search"
        },
        "ds_XtgOToUG": {
            "name": "Mitre",
            "options": {
                "query": "| from datamodel Risk.All_Risk | rename normalized_risk_object AS dest_ip | search dest_ip = $dest_ip$\n| stats\n    count  by annotations.mitre_attack.mitre_technique \n| head 5\n| rename annotations.mitre_attack.mitre_technique as \"Mitre Techniques\""
            },
            "type": "ds.search"
        },
        "ds_YybVUCKh": {
            "name": "affect_hosts_count",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| search threat = \"$threat_token$\"\n| stats dc(dest_ip)"
            },
            "type": "ds.chain"
        },
        "ds_cU42yLz2": {
            "name": "file_events_per_day",
            "options": {
                "query": "`cisco_secure_firewall` EventType=FileEvent src_ip=\"$dest_ip$\" \n| bin _time span=1d \n| stats count AS event_count BY _time \n| timechart span=1d sum(event_count) AS \"File Events per Day\""
            },
            "type": "ds.search"
        },
        "ds_f4HzVHsa": {
            "name": "description",
            "options": {
                "query": "| inputlookup threat_snort_count \n| search threat=\"$threat_token$\" \n| table description"
            },
            "type": "ds.search"
        },
        "ds_fwElVms9": {
            "name": "unique_attackers_per_day",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| search threat=\"$threat_token$\" dest_ip=\"$dest_ip$\"\n| timechart span=1d dc(src_ip) AS unique_attacker_count"
            },
            "type": "ds.chain"
        },
        "ds_i58D84BP": {
            "name": "lookup_threat_actors",
            "options": {
                "enableSmartSources": true,
                "query": "| inputlookup cisco_snort_ids_to_threat_mapping | stats count by threat | table threat"
            },
            "type": "ds.search"
        },
        "ds_m3IAfEaF": {
            "name": "snort_evernts_per_actor",
            "options": {
                "extend": "ds_mGcuwEO0",
                "query": "| search threat=\"$threat_token$\"\n| eval _time=strftime(_time, \"%Y-%m-%d\")"
            },
            "type": "ds.chain"
        },
        "ds_mGcuwEO0": {
            "name": "parent_base_search",
            "options": {
                "query": "`cisco_secure_firewall` EventType=IntrusionEvent \n| stats count AS total_alerts, dc(signature_id) AS sig_count, values(SnortRuleGroups) AS snort_rule_groups, values(connection_id) AS connection_id, values(rule) AS rule, values(dest_port) AS dest_port, values(transport) AS transport, values(app) AS app, values(signature) AS signature, values(src_ip) AS src_ip BY _time dest_ip signature_id\n| lookup cisco_snort_ids_to_threat_mapping signature_id OUTPUT threat, category, message\n| where isnotnull(threat)\n| bin _time span=1d\n| stats count AS Total_Alerts, dc(signature_id) AS sig_count, values(signature_id) AS signature_id, values(category) AS category, values(message) AS message, values(snort_rule_groups) AS snort_rule_groups, values(connection_id) AS connection_id, values(rule) AS rule, values(dest_port) AS dest_port, values(transport) AS transport, values(app) AS app, values(signature) AS signature, values(src_ip) AS src_ip BY _time dest_ip threat\n| lookup threat_snort_count threat OUTPUT description, distinct_count_snort_ids\n| table _time, dest_ip, src_ip, threat, category, message, description, signature_id, signature, snort_rule_groups, sig_count, distinct_count_snort_ids, connection_id, rule, dest_port, transport, app\n| where sig_count >= distinct_count_snort_ids",
                "queryParameters": {
                    "earliest": "$global_time.earliest$",
                    "latest": "$global_time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_q9NVolej": {
            "name": "volume_outbound",
            "options": {
                "query": "`cisco_secure_firewall` EventType=ConnectionEvent dest_ip=\"$dest_ip$\"\n| stats sum(ResponderBytes) AS outbound_bytes\n| eval Volume_Outbound_GB = round(outbound_bytes/1024/1024/1024, 2) . \" Gb\"\n| fields Volume_Outbound_GB"
            },
            "type": "ds.search"
        },
        "ds_ta3O0H2Z": {
            "name": "map_connection_events",
            "options": {
                "query": "`cisco_secure_firewall` EventType=ConnectionEvent dest_ip = $dest_ip$\n| iplocation src_ip\n| geostats count BY src_ip"
            },
            "type": "ds.search"
        },
        "ds_tuaeoaIh": {
            "name": "all_intrusion_events",
            "options": {
                "query": "`cisco_secure_firewall` EventType=IntrusionEvent dest_ip = $dest_ip$\n| bin _time span=1d \n| stats count as intrusion_count\n    by _time"
            },
            "type": "ds.search"
        },
        "ds_wzZtYKW7": {
            "name": "investigating_host",
            "options": {
                "query": "| makeresults | eval affected_ip = \"$dest_ip$\" | table affected_ip"
            },
            "type": "ds.search"
        },
        "ds_z10zC280": {
            "name": "file_events",
            "options": {
                "extend": "ds_mGcuwEO0",
